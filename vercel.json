{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "pnpm run vercel:build",
  "installCommand": "pnpm run vercel:install",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    }
  ],
  "outputDirectory": "apps/web/.next"
}
