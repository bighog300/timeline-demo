{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "pnpm run vercel:build",
  "installCommand": "pnpm run vercel:install",
  "framework": "nextjs",
  "rewrites": [
  {
    "source": "/api/:path*",
    "destination": "https://timeline-demo-2ue2e27s6-craigs-projects-bffb1f7c.vercel.app/api/:path*"
  }
],

  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    }
  ],
  "outputDirectory": "apps/web/.next"
}
